{
  "name": "@bean-tools/voicemod-websocket",
  "version": "0.1.3",
  "description": "Websocket implementation for the Voicemod Control API",
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "./test/"
  ],
  "exports": {
    ".": {
      "import": "./outDir/src/websocket.js",
      "require": "./outDir/src/websocket.js"
    }
  },
  "files": [
    "outDir/",
    "LICENSE",
    "package.json"
  ],
  "types": "./outDir/src/websocket.d.ts",
  "homepage": "https://github.com/Bean-Tools/voicemod-websocket",
  "repository": "https://github.com/Bean-Tools/voicemod-websocket.git",
  "bugs": "https://github.com/Bean-Tools/voicemod-websocket/issues",
  "author": "Izzy <360964+izzy@users.noreply.github.com>",
  "contributors": [],
  "keywords": [
    "voicemod",
    "websocket",
    "api"
  ],
  "readmeFilename": "README.md",
  "license": "GPL-3.0+",
  "private": false,
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "build-dev": "tsc -p tsconfig-dev.json",
    "test": "ts-node --preferTsExts ./test/test.ts"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/ws": "^8.5.5",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}